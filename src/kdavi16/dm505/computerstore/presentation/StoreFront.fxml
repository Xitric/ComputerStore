<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="640.0" prefWidth="960.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kdavi16.dm505.computerstore.presentation.StoreFrontController">
	<stylesheets>
		<URL value="@storefront.css" />
	</stylesheets>
   <children>
      <TabPane prefHeight="640.0" prefWidth="960.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab text="Print Lists">
               <content>
                  <BorderPane>
                     <center>
                        <TableView fx:id="resultTable" BorderPane.alignment="CENTER">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding></TableView>
                     </center>
                     <bottom>
                        <VBox spacing="4.0" BorderPane.alignment="CENTER">
                           <children>
                              <FlowPane hgap="4.0" vgap="4.0">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#listStockOnAction" text="List Stock" />
                                    <Button mnemonicParsing="false" onAction="#listComputersOnAction" text="List Computers" />
                                    <Button mnemonicParsing="false" onAction="#listComputerPricesOnAction" text="List Computer Prices" />
                                 </children>
                              </FlowPane>
                              <HBox>
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#listComponentPricesOnAction" text="List Component Prices" />
                                    <ComboBox fx:id="componentSelector" />
                                 </children>
                              </HBox>
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </VBox>
                     </bottom>
                  </BorderPane>
               </content>
            </Tab>
          <Tab text="Purchase Component">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                     </center>
                     <bottom>
                        <BorderPane BorderPane.alignment="CENTER">
                           <right>
                              <FlowPane alignment="TOP_RIGHT" hgap="48.0" vgap="4.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <TextField promptText="Enter quantity" />
                                    <Button mnemonicParsing="false" styleClass="raised" text="Purchase" />
                                 </children>
                              </FlowPane>
                           </right>
                           <left>
                              <Button mnemonicParsing="false" text="Refresh" BorderPane.alignment="CENTER" />
                           </left>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </BorderPane>
                     </bottom>
                  </BorderPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
